<% if patterns == ""%>
<p>No saved patterns</p>

<% elsif patterns %>
<section class="gallery">
    <div class="gallery-images">
    <% all_patterns.each do |index|%>
        <%patterns.split(',') %>
        <% if patterns.include?(index['id']) %>
            <a href="/patterns/details/<%= index["id"]%>"><img src="<%= index['display_image_url']%>" alt=""></a>
        <% end  %>
    <% end  %>
    </div>
</section>
<% end %>
<a href="javascript:history.back()">Go Back</a>